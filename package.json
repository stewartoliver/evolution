{
  "name": "app",
  "private": true,
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.5",
    "@rails/ujs": "^7.1.3-4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "axios": "^1.7.7",
    "chart.js": "^4.4.4",
    "chartjs": "^0.3.24",
    "chartkick": "^5.0.1",
    "date-fns": "^4.1.0",
    "esbuild": "^0.20.2",
    "hoist-non-react-statics": "^3.3.2",
    "lucide-react": "^0.453.0",
    "pnpm": "^9.11.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartkick": "^0.5.3",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "react-type-animation": "^3.2.0"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --loader:.js=jsx && yarn build:css",
    "watch": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --watch --loader:.js=jsx && yarn build:css:watch",
    "build:css": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css",
    "build:css:watch": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --watch"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.12"
  }
}
